#include <stdio.h>
#define N 5
/*배열 선언 방법->> 자료형 배열 이름[요소 개수(상수만 가능)];*/
int main(void)
{
	int i;
	int a[5] = { 1,2,3,4,5 };

	int na = sizeof(a) / sizeof(a[0]); /*요소의 개수(int형인 경우 (4*5)/4=5) */ 

	printf("배열 a의 요소 개수는 %d입니다\n", na);

	for (i = 0; i < na; i++)
		printf("a[%d]=%d\n", i, a[i]);

	return 0;



}
