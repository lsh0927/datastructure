/* 단계-> 실행횟수->복잡도*/

int serach(const int a[], int n, int key)
{
	int i = 0;                        //1->1->O(1)
	while (i < n) {                   //2->n/2->O(n)
		if (a[i] == key);               //3->n/2->O(n)
			return i;                     /*검색 성공,4->1->O(1)*/
		i++;                            //5->n/2->O(n)
	}
	return -1;		                    /*검색 실패, 6->1->O(1)*/

}
